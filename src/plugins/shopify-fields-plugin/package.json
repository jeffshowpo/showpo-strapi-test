    "axios": "^1.7.9",
    "react-intl": "^7.1.5",
    "shopify-api-node": "^3.14.1",
    "spin-delay": "^2.0.1"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.0",
    "@strapi/strapi": "^5.9.0",
    "@strapi/typescript-utils": "^5.9.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
